    <variable name="mappedbyfield" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="orderbyfield" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="order" expression="enum(&quot;ASC&quot;,&quot;DESC&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ucvariable" expression="capitalize(variable)" defaultValue="" alwaysStopAt="true" />
    <variable name="ucmappedbyfield" expression="capitalize(mappedbyfield)" defaultValue="" alwaysStopAt="true" />
