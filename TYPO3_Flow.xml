  <template name="onetomanyattr" value="/**&#10; * The $variable$s&#10; *&#10; * @var \Doctrine\Common\Collections\Collection&lt;\$model$\$ucvariable$&gt;&#10; * @ORM\OneToMany(mappedBy=&quot;$mappedbyfield$&quot;)&#10; * @ORM\OrderBy({&quot;$orderbyfield$&quot; = &quot;$order$&quot;})&#10; */&#10;protected $$$variable$s;&#10;&#10;$end$$this-&gt;$variable$s = new \Doctrine\Common\Collections\ArrayCollection();&#10;&#10;/**&#10; * Adds a $variable$&#10; *&#10; * @param \$model$ $$$variable$&#10; * @return void&#10; */&#10;public function add$ucvariable$(\$model$\$ucvariable$ $$$variable$) {&#10;&#9;$$$variable$-&gt;set$ucmappedbyfield$($this);&#10;&#9;$this-&gt;$variable$s-&gt;add($$$variable$);&#10;}&#10;&#10;/**&#10; * Removes a $variable$&#10; *&#10; * @param \$model$\$ucvariable$ $$$variable$&#10; * @return void&#10; */&#10;public function remove$ucvariable$(\$model$\$ucvariable$ $$$variable$) {&#10;&#9; $this-&gt;$variable$s-&gt;removeElement($$$variable$);&#10;}&#10;&#10;/**&#10; * Returns all $variable$s&#10; *&#10; * @return \Doctrine\Common\Collections\Collection&lt;\$model$\$ucvariable$&gt; The $variable$s&#10; */&#10;public function get$ucvariable$s() {&#10;&#9; return $this-&gt;$variable$s;&#10;}" description="Collection attribute with all neccessary methods like add, remove etc." toReformat="false" toShortenFQNames="true">
